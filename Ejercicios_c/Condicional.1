// ------------- INCLUDE -------------
#include <stdio.h>
#include <stdlib.h>
// ------------- INCLUDE -------------
// ------------- PROGRAMA -------------
int main() 
{
    int a = 10;
    int b = 1;
    if (a>b)
        printf("a es mayor que b\n");
    else 
        printf("a es menor que b\n");
    int c = 10;
    int d = 10;
    if (c==d)
        printf("c es igual que d\n");
    else 
        printf("c es distinto que d\n");
    int e = 10;
    int f = 15;
    if (e>=f)
        printf("e es mayor o igual que f\n");
    else 
        printf("e es menor que f\n");
    int g = 10;
    int h = 10;
    if (g<=h)
        printf("g es menor o igual que h\n");
    else 
        printf("g es mayor que h\n");
    return 0;
}
